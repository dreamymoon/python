————User Defined Classes
class Point:
    """ Point class for representing and manipulating x,y coordinates. """

    def __init__(self):
        """ Create a new point at the origin """
        self.x = 0
        self.y = 0
    def __init__(self, initX, initY):   
       """ Adding Parameters to the Constructor """
        self.x = initX
        self.y = initY

p = Point(7,6)
